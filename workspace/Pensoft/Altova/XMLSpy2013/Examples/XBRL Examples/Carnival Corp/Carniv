											<newline/>
											<template subtype="xbrl-linkrole" match="http://www.carnival.com/taxonomy/role/NotesToFinancialStatementsOrganizationConsolidationAndPresentationOfFinancialStatementsDisclosureTextBlock">
												<editorproperties label-lang="inherit" label-role="inherit" label-link-role="inherit"/>
												<children>
													<tgrid hidecols="when-body-empty" hiderows="when-body-empty" add-row-expand-collapse-buttons="1" add-column-close-buttons="1">
														<properties class="xbrl-table"/>
														<children>
															<tgridheader-cols>
																<children>
																	<tgridcol/>
																</children>
															</tgridheader-cols>
															<tgridbody-cols>
																<children>
																	<template subtype="xbrl-period" groupingtype="group-by" groupingmatch="if ( exists( xbrli:instant ) ) then 0 else if ( exists( xbrli:startDate ) ) then for $altova:duration in ( if ( xbrli:endDate castable as xs:date ) then xs:date( xbrli:endDate ) - xs:date( xbrli:startDate ) else xs:dateTime( xbrli:endDate ) - xs:dateTime( xbrli:startDate ) ) return fn:years-from-duration( $altova:duration ) * 12 + fn:months-from-duration( $altova:duration ) + fn:ceiling( fn:days-from-duration( $altova:duration ) div 30 - 0.5 ) else 9999999999" show-forever-periods="0" match="xbrli:period">
																		<sort>
																			<key match="current-grouping-key()" datatype="Number"/>
																		</sort>
																		<children>
																			<template subtype="userdefined" match="current-group()">
																				<sort>
																					<key match="if ( exists( xbrli:instant ) ) then xbrli:instant else if ( exists( xbrli:endDate ) ) then xbrli:endDate else &apos;forever&apos;" order="descending"/>
																				</sort>
																				<children>
																					<tgridcol/>
																				</children>
																				<variables>
																					<variable name="period" value="." valuetype="&lt;auto&gt;"/>
																				</variables>
																			</template>
